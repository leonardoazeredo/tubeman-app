{
  "name": "tubeman-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:migrate:create": "dotenv -e .env.local -- npx prisma migrate dev --create-only",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "prisma:zod": "dotenv -e .env.local -- npx prisma generate zod",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@googleapis/youtube": "^22.0.0",
    "@prisma/client": "^6.5.0",
    "bcryptjs": "3.0.2",
    "next": "15.2.2",
    "next-auth": "^5.0.0-beta.25",
    "slugify": "^1.6.6",
    "zod": "^3.24.2",
    "zod-prisma-types": "^3.2.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
